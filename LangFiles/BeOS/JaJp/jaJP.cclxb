		</dialogItem>
		
		<dialogItem name="VersionLabel">
			<rect left="88.0" top="34.0" right="396.0" bottom="48.0"/>
		</dialogItem>
		
		<dialogItem name="CopyrightLabel">
			<rect left="88.0" top="56.0" right="396.0" bottom="70.0"/>
