		</dialogItem>

		<dialogItem name="DeleteKeyMappingButton">
			<rect left="200.0" top="54.0" right="286.0" bottom="80.0"/>
			<label>削除</label>
		</dialogItem>

		<dialogItem name="CancelButton">
			<rect left="130.0" top="227.0" right="216.0" bottom="253.0"/>
