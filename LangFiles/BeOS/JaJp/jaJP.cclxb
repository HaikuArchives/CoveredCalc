				    +- DescriptionLabel		BTextView
				    +- OKButton				BButton
		-->
		
		<!-- design time font: Haru 11 -->
		<designFontInfo width="147.000000" height="13.186035"/>
		
		<dialogItem name="">
			<rect left="0.0" top="0.0" right="408.0" bottom="126.0"/>
