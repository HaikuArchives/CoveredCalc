				        +- DuplicateKeyMappingButton	BButton
				        +- DeleteKeyMappingButton		BButton
				    +- OKButton							BButton
				    +- CancelButton						BButton
		-->
		
		<!-- design time font: Haru 11 -->
		<designFontInfo width="147.000000" height="13.186035"/>
		
		<dialogItem name="">
			<rect left="0.0" top="0.0" right="322.0" bottom="265.0"/>
			<label>設定</label>
		</dialogItem>
		
		<dialogItem name="LangBox">
			<rect left="12.0" top="12.0" right="310.0" bottom="107.0"/>
